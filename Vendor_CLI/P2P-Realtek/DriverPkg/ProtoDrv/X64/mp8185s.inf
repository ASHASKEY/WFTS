;-------------------------------------------------------------------------
; mp8185s.INF -- NDIS Realtek WLAN Protocol Driver
;
; Copyright (c) 2010, Realtek Corporation
;-------------------------------------------------------------------------
[version]
Signature   = "$Windows NT$"
Class       = NetTrans
ClassGUID   = {4d36e975-e325-11ce-bfc1-08002be10318}
Provider    = %Msft%
DriverVer   =09/17/2008,5.1003.0917.2010
CatalogFile = mp8185s.CAT

[Manufacturer]
%Msft%=MSFT,NTx86,NTamd64

[MSFT.NTx86]
%NDISPROT_Desc%=Install_x86, MP_PACKET

[MSFT.NTamd64]
%NDISPROT_Desc%=Install_x64, MP_PACKET

;-------------------------------------------------------------------------
; Installation Section for x86
;-------------------------------------------------------------------------
[Install_x86]
AddReg=Inst_Ndi
Characteristics=0x0 ; 
CopyFiles=CpyFiles_Sys_x86

;-------------------------------------------------------------------------
; Installation Section for x64
;-------------------------------------------------------------------------
[Install_x64]
AddReg=Inst_Ndi
Characteristics=0x0 ; 
CopyFiles=CpyFiles_Sys_x64

;-------------------------------------------------------------------------
; Ndi installation support
;-------------------------------------------------------------------------
[Inst_Ndi]
HKR,Ndi,Service,,"mp8185s"
HKR,Ndi,HelpText,,%NDISPROT_HelpText%
HKR,Ndi\Interfaces, UpperRange,, noupper
HKR,"Ndi\Interfaces","LowerRange",,"ndis5,ndis4,ndis5_prot"

;-------------------------------------------------------------------------
; Service installation support for x86
;-------------------------------------------------------------------------
[Install_x86.Services]
AddService=mp8185s,,NDISPROT_Service_Inst_x86

[NDISPROT_Service_Inst_x86]
DisplayName     = %NDISPROT_Desc%
ServiceType     = 1 ;SERVICE_KERNEL_DRIVER
StartType       = 3 ;SERVICE_DEMAND_START
ErrorControl    = 1 ;SERVICE_ERROR_NORMAL
ServiceBinary   = %12%\mp8185s.sys
LoadOrderGroup  = NDIS
Description     = %NDISPROT_Desc%

[Install_x86.Remove.Services]
DelService=mp8185s,0x200

;-------------------------------------------------------------------------
; Service installation support for x64
;-------------------------------------------------------------------------
[Install_x64.Services]
AddService=mp8185s,,NDISPROT_Service_Inst_x64

[NDISPROT_Service_Inst_x64]
DisplayName     = %NDISPROT_Desc%
ServiceType     = 1 ;SERVICE_KERNEL_DRIVER
StartType       = 3 ;SERVICE_DEMAND_START
ErrorControl    = 1 ;SERVICE_ERROR_NORMAL
ServiceBinary   = %12%\mp8185s_x64.sys
LoadOrderGroup  = NDIS
Description     = %NDISPROT_Desc%

[Install_x64.Remove.Services]
DelService=mp8185s,0x200

;-------------------------------------------------------------------------
; Declare Destination Directories for file copy/deletion
;-------------------------------------------------------------------------
[SourceDisksNames]
1=%DiskDescription%,"",,

[SourceDisksFiles]
mp8185s_x86.sys=1
mp8185s_x64.sys=1


[DestinationDirs]
CpyFiles_Sys_x86    = 12    ; DIRID_DRIVERS
CpyFiles_Sys_x64    = 12    ; DIRID_DRIVERS


;-------------------------------------------------------------------------
; Files to Copy/Delete - Referenced by Install and Remove sections above for x86
;-------------------------------------------------------------------------
[CpyFiles_Sys_x86]
mp8185s.sys,,,2

;-------------------------------------------------------------------------
; Files to Copy/Delete - Referenced by Install and Remove sections above for x64
;-------------------------------------------------------------------------
[CpyFiles_Sys_x64]
mp8185s_x64.sys,,,2

[Strings]
Msft = "Microsoft"
DiskDescription = "Realtek WLAN Mass Production Protocol Program"
NDISPROT_Desc = "Realtek WLAN Mass Production Protocol Program"
NDISPROT_HelpText = "A driver to support user-mode I/O on NDIS devices"






